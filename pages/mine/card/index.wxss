page {
  background-color: #f5f5f5;
  display: flex;
  justify-content: center;
}
.box {
  width: 670rpx;
  height: 528rpx;
  background-color: #FFFFFF;
  border-radius: 8rpx;
  margin-top: 32rpx;
}
.top {
  font-size: 26rpx;
  font-family: PingFangSC;
  color: #666666;
  position: absolute;
  margin-top: 24rpx;
}
.left {
  width: 150rpx;
  height: 25rpx;
  margin-left: 25rpx;
}
.right {
  width: 110rpx;
  height: 25rpx;
  margin-left: 538rpx;
}
.image {
  width: 628rpx;
  height: 352rpx;
  margin: 68rpx 21rpx 0rpx 21rpx;
}
.button {
  width: 620rpx !important;
  height: 64rpx;
  background-color: #000000;
  border-radius: 8rpx;
  font-size: 26rpx;
  font-weight: 400;
  font-family: SourceHanSansCN;
  color: #ffffff;
  margin: 20rpx 25rpx 0rpx 25rpx;
}